install.packages("philanthropy")
data(exampleData1)
devtools::load_all()
data(exampleData1)
head(exampleData1)
set.seed(2128)
inTrain <- caret::createDataPartition(exampleData1$group, p = 3/4, list = FALSE)
trainData <- exampleData1[inTrain, ]
head(trainData)
table(inTrain)
table(trainData$)
table(trainData)
table(trainData$group)
markers <- trainData[, -1]
status <- factor(trainData$group, levels = c("not_needed", "needed"))
fit.lin <- linComb(markers = markers,
status = status,
event = "needed",
method = "scoring",
resample = "cv",
standardize = "range",
ndigits = 2, direction = "auto",
cutoff.method = "youden")
fit.lin
